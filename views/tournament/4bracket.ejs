<% include ../partials/boilerplate %>
  <div class='tournament-container'>
    <div class="first-round-container">
      <div  class="first-round east">
        <%- include('../partials/match', {p1: matches[0],p2:matches[1]}); %>
      </div>
      <div class="first-round west">
        <div>
          <%- include('../partials/match', {p1: matches[2],p2:matches[3]}); %>
        </div>
      </div>
    </div>
    <div class="final-round-container">
      <div class="final-round ">
        <% if (!matches[4] && matches[5]) { %>
        <%- include('../partials/match', {p1: matches[4],p2:matches[5]}); %>
        <% } %>
      </div>
    </div>
  </div>
<% include ../partials/end %>
